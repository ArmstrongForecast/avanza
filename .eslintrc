{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": 0,
    "semi": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "quote-props": 0,
    "no-restricted-syntax": ["off", "BinaryExpression[operator='of']"],
    "no-await-in-loop": 0,
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": ["**/tests/**/*", "**/scripts/**/*"]
    }]
  }
}
